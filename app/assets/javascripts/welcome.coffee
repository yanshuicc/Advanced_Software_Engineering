showTime=()->
  date = new Date()
  $('#showtime').text(date.getFullYear() + "-" + (date.getMonth()+1) + "-" + date.getDate() + " " + date.getHours() + ":" + date.getMinutes() + ":" + date.getSeconds())


setInterval(showTime, 1000)